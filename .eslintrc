{
  /* See all the pre-defined configs here: https://www.npmjs.com/package/eslint-config-defaults */
  "extends": "eslint:recommended",
  "parserOptions": { "ecmaVersion": 2018 },
  "env": {
    "amd": true,
    "browser": true,
    "jquery": true,
    "node": true,
    "es6": true,
    "worker": true
  },
  "ignorePatterns": ["node_modules/","public/","grunk/"],
  "rules": {

    "eqeqeq": 2,
    "comma-dangle": 1,
    "no-console": 0,
    "no-debugger": 1,
    "no-extra-semi": 1,
    "no-extra-parens": 1,
    "no-irregular-whitespace": 1,
    "no-multi-spaces":1,
    "no-undef": 1,
    "no-unused-vars": 1,
    "semi": 1,
    "semi-spacing": 1,
    "strict":1,
    "valid-jsdoc": [
      2,
      { "requireReturn": false }
    ]
  }
}
